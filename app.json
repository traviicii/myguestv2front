{
  "expo": {
    "name": "expo-router-example",
    "slug": "expo-router-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.expo-router-example"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.exporouterexample"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-build-properties",
        {
          "ios": {},
          "android": {}
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow MyGuest to access your photo library to attach appointment images.",
          "cameraPermission": "Allow MyGuest to access your camera to capture appointment images."
        }
      ],
      "expo-web-browser"
    ],
    "newArchEnabled": true,
    "experiments": {
      "typedRoutes": true
    }
  }
}
